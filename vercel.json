{
  "version": 2,
  "builds": [
    {
      "src": "public/",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    {
      "source": "/success.html",
      "destination": "/public/success.html"
    },
    {
      "source": "/failed.html",
      "destination": "/public/failed.html"
    },
    {
      "source": "/:orderId",
      "destination": "/public/loader.html"
    }
  ]
}